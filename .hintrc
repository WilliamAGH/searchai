{
  "extends": [
    "development"
  ],
  "connector": {
    "name": "local"
  },
  "ignoredUrls": [
    { "url": ".*playwright-report/.*" },
    { "domain": "playwright-report" }
  ],
  "hints": {
    "typescript-config/is-valid": "off",
    "axe/aria": [
      "default",
      {
        "aria-valid-attr-value": "off"
      }
    ],
    "apple-touch-icons": "off"
  }
}